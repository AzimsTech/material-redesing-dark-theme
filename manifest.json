{
  "manifest_version": 2,

  "name": "Material Redesign Dark Theme",
  "description": "A dark theme for the new Google Chrome browser design.",
  "version": "1.6.0",

  "theme": {
    "images": {
      "theme_ntp_background": "images/theme_ntp_background.png"
    },
    "colors": {
      "frame": [ 0, 44, 56 ],
      "frame_inactive": [ 0, 83, 106 ],
      "frame_incognito": [ 0, 44, 56 ],
      "frame_incognito_inactive": [ 0, 83, 106 ],
      "toolbar": [ 0, 70, 88 ],
      "tab_text": [195, 215, 217],
      "tab_background_text": [195, 215, 217],
      "bookmark_text": [195, 215, 217],
      "ntp_background": [ 0, 44, 56  ],
      "ntp_text": [ 42, 42, 42 ]
    },
    "tints": {
      "buttons": [ 0, 0, 1 ]
    },
    "properties": {
      "ntp_logo_alternate": 1,
      "ntp_background_repeat": "repeat"
    }
  }
}
